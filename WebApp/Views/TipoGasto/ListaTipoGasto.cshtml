@using Dominio
<div class="card-body">
    @if (ViewBag.mensaje != null)
    {
        <div class="alert alert-success alert-dismissible fade show" role="alert">
            @ViewBag.mensaje
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
    }

</div>

@foreach(TipoGasto item in ViewBag.TiposGastos){

    <p>@item.Nombre  @item.Descripcion    <a href="~/TipoGasto/EliminarTipoGasto?idTipoGasto=@item.Id">Eliminar</a>      </p>

    
}